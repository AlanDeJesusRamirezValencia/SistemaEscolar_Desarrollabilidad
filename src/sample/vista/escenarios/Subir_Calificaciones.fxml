<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controlador.SubirCalificaciones">
   <Button layoutX="14.0" layoutY="17.0" mnemonicParsing="false" onAction="#regresar" style="-fx-border-width: 0; -fx-background-color: #FFFFFF;">
      <graphic>
         <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../iconos/flecha_atras.png" />
         </ImageView>
      </graphic>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>

   <Label layoutX="60.0" layoutY="17.0" prefHeight="32.0" text="Subir Calificaciones">
      <font>
         <Font name="Segoe UI Semibold" size="24.0" />
      </font>
   </Label>

   <Label layoutX="24.0" layoutY="70.0" text="Matricula         Nombre Completo">
      <font>
         <Font name="Segoe UI Semibold" size="18.0" />
      </font>
   </Label>

   <ScrollPane layoutX="24.0" layoutY="105.0" prefHeight="446.0" prefWidth="582.0" style="-fx-background-color: #FFFFFF;">
      <VBox fx:id="vInformacionEstudiante" prefHeight="468.0" prefWidth="580.0" style="-fx-background-color: #FFFFFF; -fx-border-width: 0;" />
   </ScrollPane>


   <ScrollPane layoutX="690.0" layoutY="105.0" prefHeight="446.0" prefWidth="82.0" style="-fx-background-color: #FFFFFF;">
      <VBox fx:id="vAsignarCalificaciones" prefHeight="468.0" prefWidth="80.0" style="-fx-background-color: #FFFFFF; -fx-border-width: 0;" />
   </ScrollPane>


   <Button layoutX="698.0" layoutY="550.0" mnemonicParsing="false" onAction="#guardarCalificaciones" style="-fx-background-color: #FFFFFF; -fx-border-width: 0;" text="Guardar">
      <font>
         <Font name="Segoe UI Semibold" size="18.0" />
      </font>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>

   <Label layoutX="682.0" layoutY="70.0" text="Calificación">
      <font>
         <Font name="Segoe UI Semibold" size="18.0" />
      </font>
   </Label>

   <Button fx:id="btnUsuario" layoutX="656.0" layoutY="17.0" mnemonicParsing="false" onAction="#mostrarBtnCerrarSesion" prefHeight="1.0" prefWidth="144.0" style="-fx-border-width: 0; -fx-background-color: #FFFFFF;" text="admin">
      <font>
         <Font name="Segoe UI Semibold" size="24.0" />
      </font>
      <graphic>
         <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../iconos/usuario.png" />
         </ImageView>
      </graphic>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>
   <Button fx:id="btnCerrarSesion" layoutX="656.0" layoutY="53.0" mnemonicParsing="false" onAction="#cerrarSesion" style="-fx-background-color: #FFFFFF; -fx-border-width: 0;" text="Cerrar Sesión" visible="false">
      <font>
         <Font name="Segoe UI Semibold" size="18.0" />
      </font>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>

</AnchorPane>
